<tr id="<%=object.path%>">
	<td style="float:left;">
		<div style="float: left;">
		<%object.nesting.times do %>&nbsp;&nbsp;&nbsp;&nbsp;<%end%>
		</div>	
		<div style="width: 10px;float: left;">
			<% if object.expanded && !object.loaded && !object.error %>			
				<%= image_tag('tiny_loading.gif') %>
			<% else %>
			<%= link_to_remote(object.expanded ? image_tag('minus.png') : image_tag('plus.png'),
				:url => {:action => 'toggle_tree_object', :tree_object_path => object.path})%>
			<% end%>
		</div>
		<div style="float: left;">

		<%=image_tag('server.png')%>
	
		<%=object.name%>

		<% if object.error %>
			(<%=_('Error')%>: <%= object.error %>)
		<% end %>
		</div>
	</td>

	<td>
		Agent
	</td>
</tr>
